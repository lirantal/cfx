{
  "name": "r2put",
  "version": "1.0.1",
  "description": "Futuristic CLI tool for uploading files to Cloudflare R2",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "r2put": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "preview": "tsx src/dev-preview.tsx",
    "lint": "echo 'No linting configured'",
    "test": "echo 'No tests configured'",
    "clean": "rimraf dist .turbo"
  },
  "keywords": [
    "cloudflare",
    "r2",
    "s3",
    "storage",
    "cli",
    "upload"
  ],
  "author": {
    "name": "Liran Tal",
    "email": "liran.tal@gmail.com",
    "url": "https://github.com/lirantal"
  },
  "license": "MIT",
  "homepage": "https://github.com/lirantal/cfkit/tree/main/packages/r2put",
  "bugs": "https://github.com/lirantal/cfkit/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lirantal/cfkit.git",
    "directory": "packages/r2put"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "dependencies": {
    "@cfkit/r2": "workspace:*",
    "ink": "^6.6.0",
    "ink-link": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",

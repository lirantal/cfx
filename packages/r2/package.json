{
  "name": "@cfkit/r2",
  "version": "1.0.1",
  "description": "High-level Cloudflare R2 storage API wrapper",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linting configured'",
    "test": "echo 'No tests configured'",
    "clean": "rimraf dist .turbo"
  },
  "keywords": [
    "cloudflare",
    "r2",
    "s3",
    "storage",
    "aws4fetch",
    "workers"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lirantal/cfkit.git",
    "directory": "packages/r2"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {}
}

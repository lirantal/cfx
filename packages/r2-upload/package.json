{
  "name": "r2-upload",
  "version": "1.0.0",
  "description": "Futuristic CLI tool for uploading files to Cloudflare R2",
  "type": "module",
  "main": "./dist/cli.js",
  "bin": {
    "r2-upload": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "preview": "tsx src/dev-preview.tsx",
    "lint": "echo 'No linting configured'",
    "test": "echo 'No tests configured'",
    "clean": "rimraf dist .turbo"
  },
  "keywords": [
    "cloudflare",
    "r2",
    "s3",
    "storage",
    "cli",
    "upload"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cfkit/r2": "workspace:*",
    "ink": "^5.2.0",
    "ink-link": "^4.1.0",
    "ink-spinner": "^5.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "@types/react": "^18.3.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  }
}
